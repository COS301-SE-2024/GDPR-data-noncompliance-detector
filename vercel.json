{
  "version": 2,
  "name": "gnd-admin",
  "builds": [
    {
      "src": "gnd-admin/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "gnd-admin/dist/gnd-admin"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
